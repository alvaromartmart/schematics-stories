{
  "name": "stories",
  "version": "0.0.0",
  "description": "Schematics for generating component stories",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "test:watch": "nodemon --ext js --exec 'jasmine src/**/*_spec.js'"
  },
  "keywords": [
    "schematics"
  ],
  "author": "alvaromartmart",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^15.0.2",
    "@angular-devkit/schematics": "^15.0.2",
    "typescript": "~4.7.2"
  },
  "devDependencies": {
    "@schematics/angular": "^15.0.2",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine": "^4.0.0",
    "nodemon": "^2.0.20"
  }
}
